<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="using:SunroomCalculatorAvalonia.ViewModels"
                    xmlns:views="clr-namespace:SunroomCalculatorAvalonia.Views"
                    x:Class="SunroomCalculatorAvalonia.Assets.ViewResources">
    <!-- Navigation Section -->
    <DataTemplate x:Key="SunroomView" DataType="{x:Type vm:MainViewModel}">
        <views:SunroomView/>
    </DataTemplate>
    <DataTemplate x:Key="ScenarioView" DataType="{x:Type vm:MainViewModel}">
        <views:ScenarioView/>
    </DataTemplate>
    <DataTemplate x:Key="FloorPlanView" DataType="{x:Type vm:MainViewModel}">
        <views:FloorPlanView/>
    </DataTemplate>
    <DataTemplate x:Key="PanelView" DataType="{x:Type vm:Input4ViewModel}">
        <views:PanelView/>
    </DataTemplate>
    <DataTemplate x:Key="EndCutView" DataType="{x:Type vm:MainViewModel}">
        <views:EndCutView/>
    </DataTemplate>
    
</ResourceDictionary>